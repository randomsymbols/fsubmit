{
    "name": "randomsymbols/fsubmit",
    "description": "Submit HTML forms from PHP",
    "homepage": "https://github.com/randomsymbols/fsubmit",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "pguardiario/phpuri": "^1.0",
        "simplehtmldom/simplehtmldom": "^2",
        "ext-curl": "*",
        "ext-http": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev"
    },
    "autoload": {
        "psr-4": {
            "Fsubmit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fsubmit\\Tests\\": "tests/"
        }
    },
    "license": "LGPL-3.0-or-later"
}
