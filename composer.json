{
    "name": "randomsymbols/fsubmit",
    "description": "Submit HTML forms from PHP",
    "homepage": "https://github.com/randomsymbols/fsubmit",
    "type": "library",
    "require": {
        "ext-curl": "*",
        "ext-openssl": "*",
        "voku/simple_html_dom": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev",
        "overtrue/phplint": "^2.1",
        "phpstan/phpstan": "^0.12.49",
        "slevomat/coding-standard": "~6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.17",
        "phpstan/phpstan-phpunit": "^0.12.16"
    },
    "autoload": {
        "psr-4": {
            "Fsubmit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fsubmit\\Tests\\": "tests/"
        }
    },
    "license": "LGPL-3.0-or-later"
}
